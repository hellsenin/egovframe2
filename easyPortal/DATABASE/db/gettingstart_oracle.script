CREATE TABLE NOTIFICATION (
	ID					NUMBER(20)		NOT NULL,
	SUBJECT 			VARCHAR2(60)	NOT NULL,
	CONTENTS			VARCHAR2(100)	NOT NULL,
	DATE_TIME			CHAR(14)		NOT NULL,
	INTERVAL			VARCHAR2(20)	NOT NULL,
	REGISTER_DATETIME   DATE  			NOT NULL,	
	REGISTER_ID      	VARCHAR2(20)  	NOT NULL,	
	UPDATE_DATETIME     DATE  			NULL,	
	UPDATE_ID        	VARCHAR2(20)  	NULL
);

ALTER TABLE NOTIFICATION
ADD CONSTRAINT NOTIFICATION_PK PRIMARY KEY (ID);